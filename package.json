{
  "name": "backbone-forms-jam",
  "description": "A flexible, customisable form framework for Backbone.JS applications.",
  "author": {
    "name": "Charles Davison"
  },
  "version": "0.12.0",
  "dependencies": {
    "backbone": ">=1.0.0"
  },
  "devDependencies": {
    "buildify": "latest",
    "underscore": "latest",
    "uglify-js": "latest"
  },
  "main": "distribution.amd/backbone-forms.js",
  "keywords": [
    "package",
    "jam",
    "backbone",
    "forms"
  ],
  "jam": {
    "main": "distribution.amd/backbone-forms.js",
    "dependencies": {
      "backbone": ">=1.0.0"
    },
    "shim": {
      "deps": ["backbone"],
      "exports": "Backbone.Forms"
    }
  }
}
